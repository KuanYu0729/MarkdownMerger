{
	"name": "vscode-markdown-merger",
	"displayName": "Markdown â†’ PDF (embed images & internal links)",
	"description": "Convert Markdown to PDF with embedded images (base64) and internal links that scroll to headings.",
	"repository": {
		"type": "git",
		"url": "https://github.com/KuanYu0729/MarkdownMerger.git"
	},
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.60.0"
	},
	"icon": "./assets/icon.png",
	"keywords": [
		"markdown",
		"pdf",
		"concat",
		"merge",
		"document"
	],
	"license": "MIT",
	"main": "./out/extension.js",
	"publisher": "Kuan-Yu",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"dev": "ts-node index.ts",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/html-pdf-node": "^1.0.2",
		"@types/jsdom": "^21.1.7",
		"@types/mime": "^3.0.4",
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.60.0",
		"@vscode/test-electron": "^2.5.2",
		"html-pdf-node": "^1.0.8",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"jsdom": "^22.0.0",
		"markdown-it": "^13.0.1",
		"markdown-it-anchor": "^8.6.6",
		"mime": "^3.0.0"
	},
	"activationEvents": [
		"onCommand:markdownToPdf.convert"
	],
	"contributes": {
		"commands": [
			{
				"command": "markdownToPdf.convert",
				"title": "MarkdownMerger: Export to PDF"
			}
		]
	}
}